    function getCsrfToken() {
    // Tour app
    var csrfTags = Array.from(document.getElementsByTagName('meta')).filter(function(tag) {
        return tag.getAttribute('name') === 'x-csrf-token';
    });

    if (csrfTags.length === 1) {
        return csrfTags[0].getAttribute('content');
    }

    // Other apps
    csrfTags = Array.from(document.getElementsByTagName('meta')).filter(function(tag) {
        return tag.getAttribute('name') === 'x-owp-csrf-token';
    });
    if (csrfTags.length === 1) {
        return csrfTags[0].getAttribute('content');
    }

    return '';
}

/**
 * If we're in a webview, retrieve the authToken from the JS bridge and set the authorization header.
 * webView.getAuthToken() is defined by the Android JS bridge
 */
function getCsAuthHeaders() {
    var headers = {
        'x-csrf-token': getCsrfToken()
    };

    if (window.webView && window.webView.getAuthToken) {
        headers['Authorization'] = "Bearer " + webView.getAuthToken();
    }
    return headers;
}

/**
 * Only used by iOS (via stringByEvaluatingJavaScriptFromString).
 * Sets up the same webView.getAuthToken() function that we get for free from Android.
 */
window.setAuthToken = function setAuthToken(authToken) {
    window.webView = {
        getAuthToken: function() {
            return authToken;
        }
    }
};

