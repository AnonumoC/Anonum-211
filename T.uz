            setFormValue: function (formObject, fieldName, newValue) {
                newValue = newValue || "";
                if (formObject.hasOwnProperty(fieldName)) {
                    if (typeof newValue === 'string') {
                        newValue = newValue.trim();
                    }
                    formObject[fieldName] = newValue;
                }
            },

            /**
             * Updates both initial and data value of fieldName to newValue
             * @param fieldName
             * @param newValue
             */
            setInitialValue: function (fieldName, newValue) {
                this.setFormValue(this.initial, fieldName, newValue);
                this.setFormValue(this.data, fieldName, newValue);

            },

            /**
             * Sets field value and triggers validation
             * @param fieldName
             * @param newValue
             */
            setField: function (fieldName, newValue) {
                this.setFormValue(this.data, fieldName, newValue);
                delete this.errors[fieldName];
                this.validateField(fieldName);
            },

            hasErrors: function () {
                return Object.keys(this.errors).length !== 0;
            },

            /**
             * True if attachments have no errors and number of attachments is less than max amount
             * @returns {boolean}
             */
            canAttach: function () {
                return !this.errors['attachments']
                    && this.data['attachments'].length < config.maxNumberOfAttachments;
            },

            /**
             * True if form has no errors and required fields are not empty and requiredChange fields are modified
             * @returns {boolean}
             */
            canSubmit: function () {
                if (!this.hasErrors()) {
                    for (var reqField in this.requiredFields) {
                        if (this.data[this.requiredFields[reqField]].length === 0) {
                            return false;
                        }
                    }
                    for (var reqField in this.requiredChange) {
                        if (!this.isModified(this.requiredChange[reqField])) {
                            return false;
                        }

                    }
                    return true;
                }
                return false;
            },

            doSubmit: function () {
                toggleLoadingIndicator();
                var formData = utils.objectToFormData(this.data);
                var opts = {
                    dataType: 'json',
                    data: formData,
                    url: '/api/cases/create',
                    method: 'POST',
                    headers: getCsAuthHeaders(),
                    processData: false,
                    cache: false,
                    contentType: false
                };

                CSM.log(
                    CSM.constants.actions.submit,
                    CSM.constants.components.contactUs
                );

                $.post(opts).always(function (submissionResult) {
                    if (submissionResult.isSuccess === true) {
                        var successMessage = translate("message-success-alert");
                        if (typeof FormData.data.ccList !== 'undefined' && FormData.data.ccList !== '') {
                            successMessage += " " + translate("message-success-alert-cc", {ccEmailList: FormData.data.ccList});
                        }
                        successMessage += " " + generateCaseDetailsLink(submissionResult.responseBody.caseId);
                        renderPostSubmit(successMessage);
                    } else {
                        window.showTopbarNotification(submissionResult.errorMessage, '', 4);
                        // Re-enable form inputs and buttons if there was an error
                        enableFormInputs();
                        enableFormButtons();
                    }

                    // Always re-enable cancel and close buttons
                    $(".cancel-btn", containerSelector).attr('disabled', false);
                    $(".close", containerSelector).attr('disabled', false);

                    toggleLoadingIndicator();
                    triggerOnSubmitCallback(submissionResult.isSuccess)
                });
            }
        };

        /****
         *
         * ACTION LISTENERS
         *
         ****/

        var onSubmitCaseForm = function (e) {
            e.preventDefault();
            disableFormInputs();
            disableFormButtons();
            FormData.doSubmit();
            return false;
        };

        /**
         * Updates description and its initial value to template from topic if that exists
         * Sets subjectText to string value of topic
         * @param topic
         */
        var updateMessageTemplate = function (topic) {
            if (topic === "cc_in_transit_trip_issue") {
                $("#optimus-cs-oscw-rating-give-feedback").hide();
                $("#case-fields").hide();
                $("#in-transit-case-fields").removeAttr("hidden");
            }
            else {
                $("#optimus-cs-oscw-rating-give-feedback").show();
                $("#case-fields").show();
                $("#in-transit-case-fields").attr("hidden",true);
            }
            var template = DEFAULT_TEMPLATES[topic] ? DEFAULT_TEMPLATES[topic] : '';
            $('#case-description-text').val(template);
            FormData.setInitialValue("description", template);
            $("#issue-description-form-group").removeClass("has-error");

            var subjectTopic = SUBJECT_TOPICS[topic]
                || $("#case-topic option:selected").text();

            FormData.setField("subjectText",
                utils.buildSubject(subjectTopic,
                    $("#case-carrier-scac").val(),
                    $("#case-program-type").val(),
                    $("#case-country-code").val(),
