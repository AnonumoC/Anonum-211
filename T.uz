    <script>

        function logNexusEvent(actionType, subComponentType, metadata) {
            if (window.optimus && window.optimus.logNexusEvent && typeof window.optimus.logNexusEvent === "function") {
                window.optimus.logNexusEvent({
                    componentType: "R4C-NAV-BAR",
                    subComponentType: subComponentType,
                    actionType: actionType,
                    metadata: metadata
                });
            }
        }

        // Flags to check that we do not make backend call if window not in focus.
        window.optimusFlags = window.optimusFlags || {};
        window.optimusFlags.isCommunicationIconInFocus = true;
        window.optimusFlags.isCommunicationsCallDue = false;

        function refreshIcon(summaryTypeCountMap) {
            let taskCount = summaryTypeCountMap["TASK"]
            let notificationCount = summaryTypeCountMap["NOTIFICATION"]
            document.getElementById("envelop-icon").textContent = taskCount == 0 ? "" : taskCount;
            document.getElementById("bell-icon").textContent = notificationCount == 0 ? "": notificationCount;
        }

        function updateNotificationsCount() {
            if (window.optimusFlags.isCommunicationIconInFocus) {
                let communicationIcon = JSON.parse(window.localStorage.getItem("CommunicationData"));
                if(communicationIcon != null && communicationIcon.hasOwnProperty("time") && communicationIcon.hasOwnProperty("taskCount") &&
                    communicationIcon.hasOwnProperty("notificationCount") && new Date().getTime() - communicationIcon.time < 300000) {
                    // If data is found in localStorage and is not older than the refresh time the reusing data from localStorage
                    document.getElementById("envelop-icon").textContent = communicationIcon.taskCount == 0 ? "" : communicationIcon.taskCount;
                    document.getElementById("bell-icon").textContent = communicationIcon.notificationCount == 0 ? "" : communicationIcon.notificationCount;
                } else {
                    // If data is not found in localStorage or is  older than the refresh time then fetching data from backend
                    $.ajax({
                        url: '/api/rcc/getCommunicationSummary?communicationType=TASK&communicationSummaryType=COMMUNICATION_TYPE',
                        type: 'GET',
                        headers: {'x-csrf-token': getCsrfToken()},
                        contentType: 'application/json; charset=UTF-8',
                        success: function (notifications) {
                            refreshIcon(notifications.summaryTypeCountMap);
                            // Store data in local storage for reuse
                            let data = {}
                            data["time"] = new Date().getTime();
                            data["taskCount"] = document.getElementById("envelop-icon").textContent;
                            data["notificationCount"] = document.getElementById("bell-icon").textContent;
                            window.localStorage.setItem("CommunicationData", JSON.stringify(data));
                        }
                    });
                }
            } else {
                window.optimusFlags.isCommunicationsCallDue = true;
            }
        }

        $(document).ready(function () {
            updateNotificationsCount();
            // TODO: Move this time(30000) to fetch it from backend
            window.setInterval(updateNotificationsCount, 300000);
        });

        $(window).blur(function() {
            window.optimusFlags.isCommunicationIconInFocus = false;
        });
        $(window).focus(function() {
            window.optimusFlags.isCommunicationIconInFocus = true;
            if (window.optimusFlags.isCommunicationsCallDue) {
                window.optimusFlags.isCommunicationsCallDue = false;
                updateNotificationsCount();
            }
        });

    </script>

