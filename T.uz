            $(uploadFileHiddenInputId, containerSelector)
                .off("change")
                .on("change", onUploadFile);

            $(containerSelector).delegate("li.case-attachment-item i.remove", "click", onRemoveFile);
        };

        /****
         *
         * FORM UTILITIES
         *
         ****/
        var utils = {
            /**
             * Shortens fileName to fit display
             * @param fileName
             * @return display file name
             */
            getDisplayFileName: function (fileName, maxLength) {
                if (fileName.length > maxLength) {
                    var idx = fileName.lastIndexOf(".");
                    var ext = (idx !== -1) ? fileName.substring(idx) : '';
                    var name = fileName.split(".")[0];
                    return name.substring(0, maxLength) + "\u2026" + ext; // unicode for '...'
                }
                return fileName;
            },

            /**
             * Builds default subject
             * @param topic selected topic
             * @param programType carrier program type
             * @param scac carrier SCAC
             * @param countryCode ISO country code of carrier
             * @param week number of week in the year
             */
            buildSubject: function (topic, scac, programType, countryCode, week) {
                return "[" + topic + "]" +  "[" + utils.toTitleCase(programType) + "]" + "[" + scac + "]" + "[" + countryCode +  "]" + "[Week " + week + "]";
            },

            /**
             * Converts string to title case
             * @param input string
             * @return string converted to title case
             */
            toTitleCase: function (input) {
                if (input) {
                    return input.charAt(0).toUpperCase() + input.substr(1).toLowerCase();
                }
                return input;
            },

            /**
             * Converts inputObject to form data
             * @param inputObject
             * @return formData
             */
            objectToFormData: function (inputObject) {
                var formData = new window.FormData();

                for (var field in inputObject) {

                    var type = typeof inputObject[field];

                    if (type === 'string' && inputObject[field]) {
                        formData.append(field, inputObject[field]);
                    }
                    if (type === 'array' && inputObject[field]) {
                        for (var key in inputObject[field]) {
                            if (inputObject[field][key]) {
                                formData.append(field, inputObject[field][key]);
                            }
                        }
                    }
                    if (type === 'object' && inputObject[field]) {
                        for (var key in inputObject[field]) {
                            if (inputObject[field][key]) {
                                formData.append(field + "[" + key + "]", inputObject[field][key]);
                            }
                        }
                    }
                }
                return formData;
            }
        };

        /****
         *
         * FORM VALIDATION
         *
         ****/

        var validationHelper = {

            /**
             * resolves a filename into an extension to use in validation against settings array of extensions.
             * returns empty string if no extension found.
             */
            getFileExtensionFromFilename: function (filename) {
                var lastDot = filename.lastIndexOf(".");
                return (lastDot > 0 && lastDot < filename.length - 1) ?
                    filename.substring(lastDot + 1).toLowerCase() :
                    "";
            },

            /**
             * validates email address
             * @param email
             * @return true if email is valid
             */
            isEmail: function (email) {
                return /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(email);
            },

            /**
             * validates attachment type and returns true if valid
             * @param filename
             * @return boolean
             */
            isValidFileType: function (filename) {
                var ext = this.getFileExtensionFromFilename(filename);
                return ext !== "" && config.acceptedFileTypes.includes(ext);
            },

            /**
             * validates total size of all attachments and returns true if valid
             * @param files
             * @return boolean
             */
            isValidAttachmentSize: function (files) {
                var totalSize = 0;
                for (var file in files) {
                    totalSize += files[file].size;
                }
                return totalSize <= config.maxAttachmentsSize;
            }
        };

        /**
         * resets all the input values of text fields,
         * files and removes all errors
         */
        function resetForm() {
            // Reset hidden inputs that map to form fields via name, text input, files and select
            $("input:hidden[name], input:text, input:file, select, textarea", containerSelector).val('');
            $(attachmentListId, containerSelector).empty();
            $(".form-group", containerSelector).removeClass("has-error");
            $(addFileButtonId, containerSelector).show();
            $("#optimus-cs-oscw-rating-give-feedback").show();
            $("#case-fields").show();
            $("#in-transit-case-fields").attr("hidden",true);
        }

        /**
         * Reads description attributes from cases.jsp that are passed in from the backend
         */
        function setInitialDescriptionAttributes() {
            $("input[data-description-attribute]", containerSelector).each(function() {
                FormData.data.descriptionAttributes[$(this).data("description-attribute")] = $(this).val();
            });

        }

        var toggleFormInputs = function (setEnabled) {
            $("input, textarea", containerSelector).attr('disabled', !setEnabled);
            $(addFileButtonId, containerSelector).attr('disabled', !setEnabled);
        };

        var toggleFormButtons = function (setEnabled) {
            $("button", containerSelector).attr('disabled', !setEnabled);
            // always keep the Trips Redirection Button enabled
            $("tripsRedirectButtonId",containerSelector).attr('disabled', false);
        };

        var disableFormInputs = function () {
            toggleFormInputs(false);
        };

        var enableFormInputs = function () {
            toggleFormInputs(true);
        };

        var disableFormButtons = function () {
            toggleFormButtons(false);
        };

        var enableFormButtons = function () {
            toggleFormButtons(true);
        };

        /**
         * Toggles loading indicator to be hidden/shown
         */
        var toggleLoadingIndicator = function () {
            $(loadingIndicatorId).toggleClass("d-none");
        };

        var initialize = function () {
            resetForm();
            FormData.resetData();
            if (!FormData.topic) {
                disableFormInputs();
            }
            attachModalEvents();
            loadConfiguration();
            setInitialDescriptionAttributes();
            return this;
        };

        $('#cases-modal').on('show.bs.modal', function () {
            initialize();
            CSM.log(
                CSM.constants.actions.show,
                CSM.constants.components.contactUs
            );
        });

        return this;
    };
}(window.CaseManagement, window.OCS.i18n.translate, window.OCS.CSM));

