}

function hideAllRatingMessages() {
    $('.rating-message').hide();
}

function ratingClick(e) {
    var rating = (e.target.id).slice(-1);
    setRating(rating);
    showRatingMessage(rating);
}

var submitReviewCallStarted = false;

function sendReview() {
    window.OCS.CSM.log(
        OCS.CSM.constants.actions.submit,
        OCS.CSM.constants.components.review);

    if (submitReviewCallStarted) {
        return;
    } else {
        submitReviewCallStarted = true;
    }

    $('#rating-btn').prop('disabled', true);
    var data = serializeFeedback();

    dismissConnectionsForUser();
    // Closing down both modals, doesn't matter if one was open or not.
    setAllModalOpenStatusToFalse();

    var xhr = postRequest('/api/customerservice/submitReview', data);
    xhr.onreadystatechange = function() {
        if (this.readyState === XMLHttpRequest.DONE){
            submitReviewCallStarted = false;
            if (this.status === 200) {
                $('#close-rating-btn').click();
                showTopbarNotification($('#optimus-cs-ocsw-rating-success-message').select().text(), '', 5);
            } else {
                location.reload();
            }
            $('#rating-modal .rating, #rating-modal .rating-prompt').hide();
        }
    };
}

function serializeFeedback() {
     var stars = parseInt($('#rating-form input[name="stars"]').val());
     var category = $('#topic').val();
     if (!category) {
         category = '';
     }
     var feedbackText = $('#feedback-text').val();
     var questionId = '';
     if (isConnectionModalOpen()) {
         questionId = getConnectionsQuesId();
     }
     var feedback = { "stars": stars, "category": category, "text": feedbackText, "questionId": questionId};
     return feedback;
 }

function showRatingMessage(rating) {
    if(rating > 0 && rating <= 5) {
        hideAllRatingMessages();
        if (rating <=3) {
            $('#star-message-pos').show();
        }else {
            $('#star-message-neg').show();
        }
    }
}

function setRating(rating){
    $('#rating-form input[name="stars"]').val(rating);
    $(".star").removeClass('gold-star');

    if(rating > 0){
        for(var i = 1; i <= rating; i++){
            $('#star-' + i).addClass('gold-star');
        }

        $('.cs-rating-prompt').hide();

        $("#rating-btn").prop('disabled', false);
    } else {
        $("#rating-btn").prop('disabled', true);
    }
}

/**
 * Show/Hide connection question based on isConnectionModalOpen(), if there is no connection
 * survey scheduled for today, hide question div from modal, and adjust modal accordingly.
 */
function initModal(isConnectionModalRequest) {
    if (isConnectionModalRequest) {
        $('#connections-question').show();
