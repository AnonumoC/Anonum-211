function isShowConnection() {
    return isTrue(getStringFromMetadata('showConnection')) &&
        !isEqualString(getConnectionsONSId(), getLocalStorageValue(IS_CONNECTION_ACKNOWLEDGED_KEY));
}

function getConnectionsONSId() {
    return getStringFromMetadata('connectionONSId');
}

function getConnectionsQuesId() {
    return getStringFromMetadata('connectionQuesId');
}

function getStringFromMetadata(attributeName) {
    return safeTrim(document.getElementById('connections-metadata').getAttribute(attributeName));
}

function isTrue(value) {
    return isEqualString('true', value);
}

function isEqualString(var1, var2) {
    return safeTrim(var1) === safeTrim(var2);
}

function isEmpty(value) {
    return !value || '' === value.trim();
}

function safeTrim(value) {
    return isEmpty(value) ? '' : value.trim();
}

function setAllModalOpenStatusToFalse() {
    isFeedbackModalCurrentlyOpen = false;
    isConnectionModalCurrentlyOpen = false;
}

/**
 * Acknowledges / Dismiss Adhoc notifications for user.
 * We are currently using it to dismiss R4C Connections once user provides feedback.
 */
function dismissAdhocNotification(notificationId) {
    const data = {
      clientNotificationId: notificationId
    };
    var response = postRequest('/api/ons/v1/notifications/dismiss-adhoc', data);
    response.onreadystatechange = function() {
        if (this.readyState === XMLHttpRequest.DONE){
            return this.status;
        }
    }
}

function postRequest(url, reqBody) {
    var xhr = new XMLHttpRequest();
    xhr.open("POST", url, true);
    xhr.setRequestHeader("Content-Type", "application/json;charset=UTF-8");
    xhr.setRequestHeader('credentials', 'same-origin');
    xhr.setRequestHeader('x-csrf-token', getCsrfToken());

    xhr.send(JSON.stringify(reqBody));

    return xhr;
}

/**
 * Decides if connection modal is currently open or not. If connection modal is open, then all actions
 * and text should be adjusted to facilitate connections, else setup "Give Feedback" modal.
 */
function isConnectionModalOpen() {
    var showConnectionToday = isShowConnection();
    return showConnectionToday && isConnectionModalCurrentlyOpen;
}

/**
 * Dismisses connection modal for user, once dismissed, modal should not be visible to user for same question.
 */
function dismissConnectionsForUser() {
    if (isConnectionModalOpen()) {
        const CONNECTION_ONS_ID = getConnectionsONSId();
        setLocalStorageValue(IS_CONNECTION_ACKNOWLEDGED_KEY, CONNECTION_ONS_ID);
        dismissAdhocNotification(CONNECTION_ONS_ID);
    }

    // Resetting both modal's open status, doesn't matter which one was open.
    setAllModalOpenStatusToFalse();
}

function openConnectionModal() {
