window.OCS_CSM = (function (global, document) {
    "use strict";

    var csm = {};

    csm.constants = {

        pageType: 'customer_service',

        actions: {
            show: 'show',
            hide: 'hide',
            back: 'back',
            select: 'select',
            submit: 'submit'
        },

        components: {
            // The sidebar itself; actions:
            //  - "show" : The "[?] Support Center" button that opens the sidebar.
            //  - "hide" : The "X" button that closes the sidebar.
            //  - "back" : The "<-" button that goes back in the sidebar.
            sidebar: 'sidebar',

            // An FAQ topic; actions:
            //  - "show" : Click the topic link to show the FAQ items.
            faqTopic: 'faq_topic',

            // An FAQ item; actions:
            //  - "show" : Clicking the question to expand the answer.
            faq: 'faq',

            // The contact us form; actions:
            //  - "show"   : The "Feedback" link that opens the form.
            //  - "submit" : The "Submit" button that sends the form.
            //  - "select" : When choosing a new topic in the dropdown.
            contactUs: 'contact_us',

            // The about us link; actions:
            //  - "show" : The "About" link.
            about: 'about',

            // The review dialog; actions:
            //  - "show"   : The "Give Feedback" link that opens the form.
            //  - "submit" : The "Submit" button that sends the form.
            //  - "select" : When choosing a new topic in the dropdown.
            review: 'review'
        },

        subcomponents: {
            // A contact or feedback topic; for components:
            //  - "contact_us"
            //  - "feedback"
            topic: 'topic'
        }
    };

    csm.init = function () {
        // There is no need to initialize it several times
        if (!csm._config) {
            csm._config = global.OCS_CSM_Config ?
                global.OCS_CSM_Config :
                {
                    producerId: 'optimus_owp',
                    schemaId: 'optimus.ClientSideMetrics.6',
                    marketplaceId: 'DummyMarket_Beta'
                }; // For developer installations without server
        }
    };

    /**
     * @return {String} Session id from the cookie.
     */
    csm.getSessionId = function () {
        var sessionId = ['sess-id-towpus=', 'sess-id-owpus='];
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i += 1) {
            var c = ca[i].trim();
            for (var j = 0; j < sessionId.length; j += 1) {
                if (c.indexOf(sessionId[j]) === 0) {
                    return c.substring(sessionId[j].length, c.length);
                }
            }
        }
        return '';
    };

    /**
     * Turns a string with no spaces/other symbols into camel case.
     *
     * @param {String} str string with no other characters.
     * @return {String} camel collapseAllLoads string
     */
    csm.toCamelCase = function (str) {
        var metricStr = str.replace(/\s+/g, '');
        return metricStr.charAt(0).concat(metricStr.slice(1).toLowerCase());
    };

    /**
     * @return {String} current active route of the url
     */
    csm.getActiveRoute = function() {
        var route = global.location.pathname;
        var matches = route.match(/state=([^&]*)/);
        return matches ? matches[1] : route;
    };

    /**
     * @return {bool} true if logNexusEvent method is available from platform.
     */
    csm.isPlatformsLogNexusAvailable = function() {
        return (
            window.optimus &&
            window.optimus.logNexusEvent &&
            typeof window.optimus.logNexusEvent === "function"
        );
    };

    /**
     * Emit some metrics datapoint to Nexus.
     */
    csm.log = function (action, component, subcomponent, metadata) {
        subcomponent = subcomponent || null;
        metadata = metadata || null;

        csm.init();

        if (csm.isPlatformsLogNexusAvailable()) {
            // If available, use platform's nexus metrics logging method.
            window.optimus.logNexusEvent({
                rootView: csm.getActiveRoute(),
                pageType: csm.constants.pageType,
                actionType: action,
                componentType: component,
                subComponentType: subcomponent,
                metadata: metadata
            });
        } else if (global.ue) {
            // In case Platform's nexus metrics logging method is not available,
            // our own logic to publish metrics to nexus.
            global.ue.event({
                userAgent: global.navigator.userAgent,
                rootView: csm.getActiveRoute(),
                sessionId: csm.getSessionId(),
                pageType: csm.constants.pageType,
                actionType: action,
                componentType: component,
                subComponentType: subcomponent,
                metadata: metadata
            },
            csm._config.producerId,
            csm._config.schemaId);
        }
    };

    /**
     * logs Critical Feature Metrics data for CSM
     * Design Wiki for CSM Metrics: https://w.amazon.com/bin/view/OptimusMM/OptimusTourAssignment/UserMetrics/Design
     */
    csm.logCriticalFeature = function () {
        if (global.ue) {
            global.ue.event('cf');
        }
    };

    return csm;

})(window, document);

