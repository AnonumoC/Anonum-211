<script>
    (function () {
        function handleFooterNotification(notification, opts) {
           if(notification.clientNotificationId != null
             && (notification.onsClientId || "").indexOf("KitKatService") >= 0) {
             if (notification.clientNotificationId.includes('FuzzyMatchNotification')) {
               onsfooter.showBannerNotification(new OptimusNotification(opts));
               document.getElementById(notification.clientNotificationId).href = notification.actionUri;
               removeExistingNotification(notification);
             } else if(notification.clientNotificationId.includes('FulFillNotification')) {
               let capacityRequestId = notification.actionUri;
               let actionText = '/tours/tours/';
               return $.ajax({
                 url: '/api/tours/loadboard/'.concat(capacityRequestId.toString()).concat('/status'),
                 type: 'GET'
               }).then(function (response) {
                 if (response.tourOrLoadId !== null) {
                    onsfooter.showBannerNotification(new OptimusNotification(opts));
                    document.getElementById(notification.clientNotificationId).href= actionText.concat(response.tourOrLoadId);
                    removeExistingNotification(notification);
                 }
               });
             }
            } else {
              onsfooter.showBannerNotification(new OptimusNotification(opts));
            }
        }

        function removeExistingNotification(notification) {
            if(document.getElementById('alert-'+notification.onsClientId+'-'+notification.clientNotificationId)) {
                document.getElementById('alert-'+notification.onsClientId+'-'+notification.clientNotificationId).remove();
            }
        }

        function filterNotification(notifications) {
           let footerNotificationThreshold = 3;
           let filteredNotifications = [];
           let footerNotificationCounter = 0;
           for(let i in notifications) {
             let n = notifications[i];
             if(n.footerNotification) {
               if(footerNotificationCounter < footerNotificationThreshold) {
                  filteredNotifications.push(n);
                  footerNotificationCounter++;
               }
             } else {
                filteredNotifications.push(n);
             }
           }
           return filteredNotifications;
        }

        // Flags to check that we do not make backend call if window not in focus.
        window.optimusFlags = window.optimusFlags || {};
        window.optimusFlags.isWindowInFocus = true;
        window.optimusFlags.isNotificationsCallDue = false;

        function pollForNotifications() {
          if (window.optimusFlags.isWindowInFocus) {
            $.ajax({
              url: "/api/ons/v1/notifications",
              type: "GET",
              success: function (notifications) {
                let filteredNotifications = filterNotification(notifications);
                for (let i in filteredNotifications) {
                  let n = filteredNotifications[i];
                  let opts = {
                    message: n.body,
                    severity: n.severity,
                    clientId: n.onsClientId,
                    clientNotificationId: n.clientNotificationId,
                    buttonURI:  n.actionUri ,
                    buttonText: n.actionText,
                    dismissible: n.dismissible,
                    createTime: n.createTime,
                    footerNotification: n.footerNotification,
                    type: OptimusNotification.Constants.Types.BANNER
                  };
                  if(n.footerNotification) {
                    handleFooterNotification(n, opts);
                  } else {
                    ons.showBannerNotification(new OptimusNotification(opts));
                  }
                }
              }
            });
          } else {
            window.optimusFlags.isNotificationsCallDue = true;
          }
        }

        pollForNotifications();

        $(window).blur(function() {
          window.optimusFlags.isWindowInFocus = false;
        });
        $(window).focus(function() {
          window.optimusFlags.isWindowInFocus = true;
          if (window.optimusFlags.isNotificationsCallDue) {
            window.optimusFlags.isNotificationsCallDue = false;
            pollForNotifications();
          }
        });

        setInterval(pollForNotifications, 30000);
    })();
</script>

