         */
        var onCaseFormChange = function () {
            var val =  $(this).val();
            $("#" + $(this).data("form-group")).removeClass("has-error");
            if ($(this).data("is-json")) { // if json field parse it
                val = JSON.parse(val);
            }
            onFieldUpdate($(this).attr("name"), val);
