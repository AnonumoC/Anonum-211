        const TIME_TO_DELAY_CONNECTION = 1 * 60 * 1000; // 1 min.
        setTimeout(initiateR4CConnection, TIME_TO_DELAY_CONNECTION); // Waiting for 2 min more if feedback modal is open.
    } else {
        isConnectionModalCurrentlyOpen = true;
        $('a[data-target="#rating-modal"]')[0].click();
        reviewInit(true);
    }
}

function initiateR4CConnection() {
    const TIME_TO_DELAY_CONNECTION = 5 * 60 * 1000; // 5 min
    var timeLeftForConnection = checkCacheForTimeSpend();
    if (timeLeftForConnection > TIME_TO_DELAY_CONNECTION) {
        updateConnectionsCookie(TIME_TO_DELAY_CONNECTION);
        timeLeftForConnection = TIME_TO_DELAY_CONNECTION;
    }
    setTimeout(openConnectionModal, timeLeftForConnection);
}

const CONNECTIONS_COOKIE_NAME = 'r4c-connection-time';
function checkCacheForTimeSpend() {
    const cookieValue = getCookieValue(CONNECTIONS_COOKIE_NAME);
    if (isEmpty(cookieValue)) {
        // Default value, if value is too high, initiateR4CConnection method updates cookie with new value.
        return 10000000000;
    }

    const currentTime = new Date();
    const timeLeftForConnection = currentTime.getTime() - cookieValue;

    return Math.abs(timeLeftForConnection);
}

function getCookieValue(cookieName) {
    var name = cookieName + '=';
    var decodedCookie = decodeURIComponent(document.cookie);
    var cookieArray = decodedCookie.split(';');
    for(var i = 0; i < cookieArray.length; i++) {
        var cookie = cookieArray[i];
        while (cookie.charAt(0) == ' ') {
          cookie = cookie.substring(1);
        }
        if (cookie.indexOf(name) == 0) {
          return cookie.substring(name.length, cookie.length);
        }
    }
    return '';
}

function updateConnectionsCookie(connectionTime) {
    var cookieExpiryTime = new Date();
    const ONE_DAY = 24*60*60*1000;
    cookieExpiryTime.setTime(cookieExpiryTime.getTime() + ONE_DAY);
    var expires = 'expires=' + cookieExpiryTime.toGMTString();

    var cookieConnectionTimeValue = new Date();
    cookieConnectionTimeValue.setTime(cookieConnectionTimeValue.getTime() + connectionTime);
    var connectionCookie = CONNECTIONS_COOKIE_NAME + '=' + cookieConnectionTimeValue.getTime() ;

    document.cookie = connectionCookie + ';' + expires + ';path=/;SameSite=Strict;';
}

function getLocalStorageValue(key) {
    return window.localStorage.getItem(key);
}

function setLocalStorageValue(key, value) {
    window.localStorage.setItem(key, value);
}

