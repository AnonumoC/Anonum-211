        function getCsrfToken() {
    // Tour app
    var csrfTags = Array.from(document.getElementsByTagName('meta')).filter(function(tag) {
        return tag.getAttribute('name') === 'x-csrf-token';
    });

    if (csrfTags.length === 1) {
        return csrfTags[0].getAttribute('content');
    }

    // Other apps
    csrfTags = Array.from(document.getElementsByTagName('meta')).filter(function(tag) {
        return tag.getAttribute('name') === 'x-owp-csrf-token';
    });
    if (csrfTags.length === 1) {
        return csrfTags[0].getAttribute('content');
    }

    return '';
}

/**
 * If we're in a webview, retrieve the authToken from the JS bridge and set the authorization header.
 * webView.getAuthToken() is defined by the Android JS bridge
 */
function getCsAuthHeaders() {
    var headers = {
        'x-csrf-token': getCsrfToken()
    };

    if (window.webView && window.webView.getAuthToken) {
        headers['Authorization'] = "Bearer " + webView.getAuthToken();
    }
    return headers;
}

/**
 * Only used by iOS (via stringByEvaluatingJavaScriptFromString).
 * Sets up the same webView.getAuthToken() function that we get for free from Android.
 */
window.setAuthToken = function setAuthToken(authToken) {
    window.webView = {
        getAuthToken: function() {
            return authToken;
        }
    }
};

        var isConnectionModalCurrentlyOpen = false;
var isFeedbackModalCurrentlyOpen = false;
const IS_CONNECTION_ACKNOWLEDGED_KEY = 'isConnectionAcknowledgedByUser';

function reviewInit(isConnectionModalRequest) {
    window.OCS.CSM.log(
        OCS.CSM.constants.actions.show,
        OCS.CSM.constants.components.review);

    window.csOriginalSize = $(window).height();

    $('.star').off('click').click(ratingClick);
    $('#rating-btn').off('click').click(sendReview);
    $('#cancel-rating-btn').off('click').click(dismissConnectionsForUser);
    $('#close-rating-btn').off('click').click(dismissConnectionsForUser);
    $('#topic').off('change').change(changeTopicSelection);

    if($('.cs-webview').length){
        $(window).resize(function() { toggleButtonPosition($('#rating-btn')); });
    }

    hideAllRatingMessages();
    resetFeedback();

    $('#rating-modal .rating').show();
    setRating(0);
    initModal(isConnectionModalRequest);
}

function resetFeedback() {
    resetTopicSelection();
    resetFeedbackText();
}

function resetFeedbackText() {
    $('#feedback-text').val('').change();
}

function resetTopicSelection() {
    var defaultValue = $("#disabled-topic-option").val();
    $('#topic').val(defaultValue).change();
}

function changeTopicSelection() {
    var id = $('#topic').val();
    if (id) {
        window.OCS.CSM.log(
            OCS.CSM.constants.actions.select,
            OCS.CSM.constants.components.review,
