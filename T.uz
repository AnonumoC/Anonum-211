            $("#hide-help-button").click(closeSidebar);
            $(".global-sneezeguard").click(closeSidebar);
            $("#customer-service-button").click(openSidebar);
            $("#customer-service-button-mobile").click(openSidebar);
            reviewInit(false);
        }

        /**
         * Open the CS sidebar and reset its content to a known good state.
         */
        function csDisplayMainSidebar() {
            if (window.faqDrawerComponent) {
                //Tell FAQ drawer react app to load its data if it exists.
                window.faqDrawerComponent.loadFaqDrawer();
            }
            $("#cs-nav-bar-title").html(csNavBarTitle);
            $('#cs-back-button').hide();
            $("#cs-body").html(csHomeBody);
            $("#faq-body").html(csFaqTopicsHtml);
            $('#customer-service').addClass('active');

            // Set up our event listeners
            $('.faq-topic').off('click').click(showFaqTopic);
            $('#cs-back-button').off('click').click(function () {
                window.OCS.CSM.log(
                    window.OCS.CSM.constants.actions.back,
                    window.OCS.CSM.constants.components.sidebar);
                csDisplayMainSidebar();
            });
            $('#cs-about').off('click').click(showAbout);
            $("#customer-service .card").off('click').click(csOpenCard);
            $('#cs-contact-us').off('click').click(function () {
               csCloseSidebar();
               window.location.replace('/cases'); // redirect to contact center page
            });
            const faqDrawerZIndex = $("#customer-service").css("z-index");
            $('#notification-area').css('z-index', faqDrawerZIndex - 1);
        }

        /**
         * Close the CS sidebar
         */
        function csCloseSidebar() {
            $('#customer-service').removeClass('active');
            $('#notification-area').css('z-index', '');
        }

        /**
         * Using padding on the <a> element does not work on Android 17 so this is the work around to still have spacing
         * around menu items on all supported devices while still having all of the menu item be clickable.
         */
        function csOpenCard(e) {
            if ($(e.target).find('a')) {
                $(e.target).find('a').click();
            }
        }
