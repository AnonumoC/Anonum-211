        };

        /**
         * When a topic is selected,
         * update CC email list with carrier group email if that exists
         * @param carrierGroupEmail
        */
        var updateCcList = function (carrierGroupEmail) {
            $('#case-cc-list').val(carrierGroupEmail);
            FormData.setInitialValue("ccList", carrierGroupEmail);
        };

        /**
         * On any field update re-evaluates if form can be submitted and displays errors
         * @param fieldName
         * @param value
         */
        var onFieldUpdate = function (fieldName, value) {
            FormData.setField(fieldName, value);
            if (FormData.canSubmit()) {
                $(".submit-btn", containerSelector).attr('disabled', false);
            } else {
                $(".submit-btn", containerSelector).attr('disabled', true);
                if (FormData.hasErrors()) {
                    for (var err in FormData.errors) {
                        var elId = "#" + $("[name='" + err + "']").data("form-group");
                        $(".form-control-feedback", $(elId)).html(FormData.errors[err]);
                        $(elId, containerSelector).addClass("has-error");
                    }
                }
            }
        };

        /**
         * This function will trigger the onSubmit callback which is store in the DOM nodess
         * @param isSuccess
         */
        var triggerOnSubmitCallback = function (isSuccess) {
            var callback = $(containerSelector).data('onSubmit');
            if (callback) {
                callback(isSuccess);
            }
        };

        /**
         * On form field change clears its error
